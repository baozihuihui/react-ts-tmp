{
    "editor.formatOnSave": true,
    "eslint.enable": true, //是否开启vscode的eslint
    "tslint.enable": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.options": { //指定vscode的eslint所处理的文件的后缀
        "extensions": [
            ".js",
            ".ts",
            ".tsx"
        ]
    },
    "eslint.exclude": "**/node_modules/**/*,**/.history/**",
}