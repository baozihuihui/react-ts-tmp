{
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": false,
				"targets": {
					"browsers": ["> 1%", "IE 10", "safari >= 7"]
				},
				"useBuiltIns": "usage",
				"corejs": 3
			}
		],
		"@babel/react"
	],
	"plugins": [
		"lodash",
		"recharts",
		// antd 按需引入
		["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
	]
}
